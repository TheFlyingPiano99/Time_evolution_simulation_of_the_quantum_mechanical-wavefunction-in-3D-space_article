@article{FEIT1982412,
	title = {Solution of the Schrödinger equation by a spectral method},
	journal = {Journal of Computational Physics},
	volume = {47},
	number = {3},
	pages = {412-433},
	year = {1982},
	issn = {0021-9991},
	doi = {https://doi.org/10.1016/0021-9991(82)90091-2},
	url = {https://www.sciencedirect.com/science/article/pii/0021999182900912},
	author = {M.D Feit and J.A Fleck and A Steiger},
	abstract = {A new computational method for determining the eigenvalues and eigenfunctions of the Schrödinger equation is described. Conventional methods for solving this problem rely on diagonalization of a Hamiltonian matrix or iterative numerical solutions of a time independent wave equation. The new method, in contrast, is based on the spectral properties of solutions to the time-dependent Schrodinger equation. The method requires the computation of a correlation function 〈ψ(r, 0)| ψ(r, t)〉 from a numerical solution ψ(r, t). Fourier analysis of this correlation function reveals a set of resonant peaks that correspond to the stationary states of the system. Analysis of the location of these peaks reveals the eigenvalues with high accuracy. Additional Fourier transforms of ψ(r, t) with respect to time generate the eigenfunctions. The effectiveness of the method is demonstrated for a one-dimensional asymmetric double well potential and for the two-dimensional Hénon-Heiles potential.}
}

@article{BLUM2022125,
	title = {The birth of quantum mechanics from the spirit of radiation theory},
	journal = {Studies in History and Philosophy of Science},
	volume = {91},
	pages = {125-147},
	year = {2022},
	issn = {0039-3681},
	doi = {https://doi.org/10.1016/j.shpsa.2021.11.004},
	url = {https://www.sciencedirect.com/science/article/pii/S0039368121001813},
	author = {Alexander S. Blum and Martin Jähnert},
	keywords = {Quantum mechanics, Quantum electrodynamics, Light quanta, Correspondence principle, Bohr-Kramers-Slater theory, Radiation},
	abstract = {This paper starts from the observation that the major hallmarks in the history of quantum mechanics centrally involved light and radiation. Consequently, we ask how precisely did concepts and ideas from radiation theory define the creation of quantum mechanics? And why was it a quantum theory of mechanics and not a quantum theory of electrodynamics that was crafted in the years 1925/26? Following these questions, this paper provides a reading of the history of the old quantum theory from the perspective of radiation. We argue that both wave and matrix mechanics, while emerging from widely different trajectories, emerged as spin-offs from nascent, ultimately unsuccessful quantum theories of radiation. We discuss the epistemological implications of this development for the historical and philosophical analysis of quantum mechanics and quantum field theory.}
}

