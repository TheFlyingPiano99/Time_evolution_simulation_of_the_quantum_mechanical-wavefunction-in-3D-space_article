@article{KOSLOFF198335,
title = {A fourier method solution for the time dependent Schrödinger equation as a tool in molecular dynamics},
journal = {Journal of Computational Physics},
volume = {52},
number = {1},
pages = {35-53},
year = {1983},
issn = {0021-9991},
